<h1>New Ingredient for <%= @dish.name %></h1>
<%= form_with(model: @ingredient,
              url: {action:"create",
                    controller: "ingredients",
                    dish_id: @dish.id},
              method: :post) do |form| %>
    <%= render 'form', { form: form, ingredient: @ingredient }  %>
<% end %>

<%= link_to 'Back', dish_ingredients_path(@dish) %>
